{
  "manifest_version": 2,
  "name": "Denotify",
  "version": "1.0",

  "description": "Remove visual notifications from websites you visit.",

  "icons": {
    "48": "icons/sh.png"
  },

  "content_scripts": [
    {
      "run_at": "document_start",
      "matches": ["<all_urls>"],
      "css": [
        "denotify.css"
      ]
    }
  ]
}
